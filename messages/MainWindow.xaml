<Window x:Class="messages.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:messages"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="MainGrid">
        <Button Content="DialogOk" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="145" Height="64" Click="Button_ClickOK"/>
        <Button Content="DialogOkCancel" HorizontalAlignment="Left" Margin="160,10,0,0" VerticalAlignment="Top" Width="145" Height="64" Click="Button_ClickOKCancel"/>
        <Button Content="DialogYes" HorizontalAlignment="Left" Margin="310,10,0,0" VerticalAlignment="Top" Width="145" Height="64" Click="Button_ClickYes"/>
        <Button Content="DialogYesNo" HorizontalAlignment="Left" Margin="460,10,0,0" VerticalAlignment="Top" Width="145" Height="64" Click="Button_ClickYesNo"/>
        <Button Content="DialogYesNoCancel" HorizontalAlignment="Left" Margin="610,10,0,0" VerticalAlignment="Top" Width="145" Height="64" Click="Button_ClickYesNoCancel"/>
    </Grid>
</Window>
